{
  "name": "veritable-ui",
  "version": "0.8.72",
  "description": "UI for Veritable",
  "main": "src/index.ts",
  "type": "module",
  "author": "Digital Catapult",
  "license": "Apache-2.0",
  "engines": {
    "node": "^22.0.0",
    "npm": "^10.0.0"
  },
  "scripts": {
    "depcheck": "depcheck",
    "test:unit": "npm run build && NODE_ENV=test ./node_modules/.bin/mocha ./build/src/**/__tests__/**/*.test.js",
    "test:integration": "npm run build && NODE_ENV=test ./node_modules/.bin/mocha ./test/**/*.test.ts",
    "test:e2e": "playwright test --ui",
    "test:playwright": "playwright test",
    "check": "npm run tsoa:build && tsc --noEmit",
    "clean": "rimraf ./build ./dist ./src/routes.ts ./src/swagger.json",
    "build": "npm run clean && npm run tsoa:build && tsc -p ./tsconfig.json",
    "build:watch": "npm run clean && concurrently \"npm run tsoa:watch\" \"tsc -p ./tsconfig.json --watch\"",
    "build:dist": "npm run clean && npm run tsoa:build && tsc -p ./tsconfig.dist.json",
    "tsoa:build": "tsoa spec-and-routes",
    "tsoa:watch": "node --watch-path ./src/controllers ./node_modules/.bin/tsoa spec-and-routes",
    "dev": "npm run build && concurrently \"npm run build:watch\" \"NODE_ENV=dev node --watch-path ./build build/src/index.js | pino-colada\"",
    "init": "node dist/init.js",
    "start": "node dist/index.js",
    "db:cmd": "node ./node_modules/.bin/knex",
    "db:migrate": "npm run db:cmd -- migrate:latest",
    "db:rollback": "npm run db:cmd -- migrate:rollback",
    "db:seed": "npm run db:cmd -- seed:run",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "xss-scan": "xss-scan"
  },
  "dependencies": {
    "@digicatapult/tsoa-oauth-express": "^0.1.42",
    "@kitajs/html": "^4.2.2",
    "@kitajs/ts-html-plugin": "^4.0.3",
    "@tsoa/runtime": "^6.4.0",
    "@types/compression": "^1.7.5",
    "argon2": "^0.41.1",
    "body-parser": "^2.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "express": "^4.21.0",
    "htmx-ext-json-enc": "^2.0.1",
    "htmx.org": "^2.0.2",
    "knex": "^3.1.0",
    "nodemailer": "^6.9.15",
    "pg": "^8.12.0",
    "pino": "^9.4.0",
    "pino-http": "^10.3.0",
    "swagger-ui-express": "^5.0.1",
    "tsoa": "^6.4.0",
    "tsyringe": "^4.8.0",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.10.0",
    "@playwright/test": "^1.47.0",
    "@types/body-parser": "^1.19.5",
    "@types/chai": "^4.3.19",
    "@types/chai-jest-snapshot": "^1.3.8",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.8",
    "@types/node": "^20.16.5",
    "@types/nodemailer": "^6.4.15",
    "@types/pino-http": "^5.8.4",
    "@types/sinon": "^17.0.3",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "chai": "^5.1.1",
    "chai-jest-snapshot": "^2.0.0",
    "concurrently": "^9.0.1",
    "depcheck": "^1.4.7",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "mocha": "^10.7.3",
    "pino-colada": "^2.2.2",
    "playwright-ctrf-json-reporter": "^0.0.18",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.0.0",
    "rimraf": "^6.0.1",
    "sinon": "^19.0.2",
    "supertest": "^7.0.0",
    "typescript": "^5.6.2",
    "undici": "^6.19.8"
  }
}
