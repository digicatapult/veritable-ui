{
  "entryFile": "src/index.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": [
    "src/controllers/**/*.tsx",
    "src/controllers/**/*.ts"
  ],
  "multerOpts": {
    "limits": {
      "fileSize": 104857600
    }
  },
  "spec": {
    "outputDirectory": "src",
    "specVersion": 3
  },
  "routes": {
    "esm": true,
    "routesDir": "src",
    "iocModule": "src/ioc"
  }
}
